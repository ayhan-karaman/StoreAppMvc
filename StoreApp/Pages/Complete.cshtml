@page
@{
    ViewData["Title"] = "Complete";
}

<div class="text-center">
    <h6>Thanks</h6>
    <p>Thanks for placing order #@OrderId</p>
    <p>We'll ship yout goods as soon as possible.</p>
    <a class="btn btn-primary" asp-action="Index" asp-controller="Home">Return to store</a>
</div>

@functions
{
    [BindProperty(SupportsGet = true)]
    public string? OrderId { get; set; }
}