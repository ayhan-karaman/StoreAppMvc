@model List<Product>
    <div class="row">
        <div class="col-3">
            @await Component.InvokeAsync("CategoryList")
        </div>
        <div class="col-9">
            <div class="row">
                @foreach(var product in Model)
                {
                
                    <partial name="ProductCard" model="product" />
                
                }
            </div>
        </div>
    </div>